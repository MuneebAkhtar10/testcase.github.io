// Start Contact section
section#mu-contact
  .container
    .row
      .col-md-12
        .mu-contact-area
          .mu-title
            span.mu-subtitle Get In Touch
            h2 Contact Us
          .mu-contact-content
            .row
              .col-md-6
                .mu-contact-left
                  // Email message div
                  #form-messages
                  // Start contact form
                  form#ajax-contact.mu-contact-form(method='post', action='mailer.php')
                    .form-group
                      label(for='name') Your Name
                      input#name.form-control(type='text', name='name', placeholder='Name', required='')
                    .form-group
                      label(for='email') Email address
                      input#email.form-control(type='email', name='email', placeholder='Email', required='')
                    .form-group
                      label(for='subject') Subject
                      input#subject.form-control(type='text', name='subject', placeholder='Subject', required='')
                    .form-group
                      label(for='message') Message
                      textarea#message.form-control(name='message', cols='30', rows='10', placeholder='Type Your Message', required='')
                    button.mu-send-btn(type='submit') Send Message
              .col-md-6
                .mu-contact-right
                  .mu-contact-widget
                    h3 Office Address
                    
                    address
                      p
                        i.fa.fa-phone
                        |  (850) 457 6688
                      p
                        i.fa.fa-envelope-o
                        | contact@markups.io
                      p
                        i.fa.fa-map-marker
                        | 368 St
                  .mu-contact-widget
                    h3 Open Hours
                    address
                      p
                        span Monday - Friday
                        |  9.00 am to 12 pm
                      p
                        span Saturday
                        |  9.00 am to 10 pm
                      p
                        span Sunday
                        |  10.00 am to 12 pm
// End Contact section
